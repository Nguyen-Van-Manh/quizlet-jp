{
  "type": "sqlite",
  "database": "nestjs",
  "synchronize": true,
  "logging": false,
    "entities": [
      "dist/**/*.entity.js"
    ],
    "migrations": [
      "dist/migrations/*.js"
    ],
    "subscribers": [
      "dist/migrations/subscriber/*.js"
    ],
    "cli": {
      "migrationsDir": "src/migrations/"
    }
  }